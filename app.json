{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photos to let you share images and videos in chat.",
        "NSCameraUsageDescription": "We need access to your camera to let you capture photos and videos in chat.",
        "NSMicrophoneUsageDescription": "We need access to your microphone to let you send voice notes."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "We need access to your camera to let you capture photos and videos."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "We need access to your photos to let you share images and videos."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "We need access to your microphone to let you send voice notes."
        }
      ],
      [
        "expo-document-picker",
        {}
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c8ab4f5d-adfd-4ea0-8c24-96df65b27dac"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c8ab4f5d-adfd-4ea0-8c24-96df65b27dac"
    },
    "android": {
      "package": "com.somuah_2708.boltexponativewind",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
